<h1 class="form">Log In</h1>
<%= form_tag sessions_path do %>
<ul>
	<li>
		<%= label_tag :username %>
		<%= text_field_tag :username, params[:username] %>
	</li>
	<li>
		<%= label_tag :password %>
		<%= password_field_tag :password %>
	</li>
	<li><%= submit_tag "Log In" %></li>
	<li>Dont have an account? <%= link_to 'Sign up', 'sign_up' %></li>
</ul>
<% end %>
